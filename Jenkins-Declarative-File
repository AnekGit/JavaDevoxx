pipeline{
    
    agent any 
    tools {
       maven : 'Maven 3.6.3'
       jdk8  : 'jdk8'
    
    }
     stages{
     
        stage("SCM Checkout"){
        
            steps{
                
                echo "Already Checked Out from Git"
                //git credentialsId: 'AnekGit', url: 'https://github.com/AnekGit/JavaDevoxx.git'
            }
            
        }//stage
        
        stage("Maven Package"){
         
         steps{
               echo "Maven Clean"
               echo "PATH = ${PATH}"
               echo "M2_HOME = ${M2_HOME}" 
         }
         
        }//stage
        
        
    }//stages
    
    
}//pipeline
