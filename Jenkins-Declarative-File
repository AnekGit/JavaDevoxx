pipeline{
    
    agent any 
    
     stages{
     
        stage("SCM Checkout"){
        
            steps{
                
                echo "Already Checked Out from Git"
                //git credentialsId: 'AnekGit', url: 'https://github.com/AnekGit/JavaDevoxx.git'
            }
            
        }//stage
        
        stage("Maven Package"){
         sh "mvn clean package"
        }
        
        
    }//stages
    
    
}//pipeline
